{
  "name": "@doyaken/doyaken",
  "version": "1.0.0",
  "description": "Autonomous AI agent CLI for software development - install once, use on any project",
  "keywords": [
    "ai",
    "agent",
    "claude",
    "autonomous",
    "automation",
    "cli",
    "developer-tools",
    "ai-agent",
    "code-generation"
  ],
  "author": "Mitchell",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doyaken/doyaken.git"
  },
  "homepage": "https://github.com/doyaken/doyaken#readme",
  "bugs": {
    "url": "https://github.com/doyaken/doyaken/issues"
  },
  "bin": {
    "doyaken": "./bin/doyaken",
    "dk": "./bin/doyaken"
  },
  "files": [
    "bin/",
    "lib/",
    "prompts/",
    "templates/",
    "config/",
    "scripts/",
    "install.sh"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "test": "bash scripts/test.sh",
    "lint": "bash scripts/lint.sh",
    "validate": "bash scripts/validate-yaml.sh",
    "check": "bash scripts/check-all.sh",
    "setup": "bash scripts/setup-hooks.sh",
    "prepublishOnly": "npm run check"
  },
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true
}
